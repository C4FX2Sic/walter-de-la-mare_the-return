		<meta property="role" refines="#transcriber-5420" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-5421">with eyes       fixed wide on the floating luminous gloom</dc:contributor>
		<meta property="file-as" refines="#transcriber-5421">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-5421" scheme="marc:relators">trc</meta>
