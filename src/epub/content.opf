		<meta property="role" refines="#transcriber-39" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-40">and</dc:contributor>
		<meta property="file-as" refines="#transcriber-40">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-40" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-41">through the gate that Influenza in       departing had left ajar</dc:contributor>
		<meta property="file-as" refines="#transcriber-41">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-41" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-42">had returned upon him.     &lt;/p&gt;     &lt;p&gt;       ‘But I suppose we are all pretty much the same</dc:contributor>
