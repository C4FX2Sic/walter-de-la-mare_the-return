		<meta property="role" refines="#transcriber-1089" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-1090">you say</dc:contributor>
		<meta property="file-as" refines="#transcriber-1090">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-1090" scheme="marc:relators">trc</meta>
