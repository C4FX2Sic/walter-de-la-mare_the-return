		<meta property="file-as" refines="#transcriber-7923">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-7923" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-7924">indeed</dc:contributor>
		<meta property="file-as" refines="#transcriber-7924">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-7924" scheme="marc:relators">trc</meta>
