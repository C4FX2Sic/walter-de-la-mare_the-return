		<meta property="role" refines="#transcriber-2673" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-2674">gazed into the shadowiness. Her       fingers lay quiet</dc:contributor>
		<meta property="file-as" refines="#transcriber-2674">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-2674" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-2675">passive in his</dc:contributor>
