		<meta property="role" refines="#transcriber-2662" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-2663">a brief pause</dc:contributor>
		<meta property="file-as" refines="#transcriber-2663">TRANSCRIBER_SORT</meta>
