		<meta property="role" refines="#transcriber-6487" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-6488">Lawford</dc:contributor>
		<meta property="file-as" refines="#transcriber-6488">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-6488" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-6489">that hasn’t at least a       glimmer of the real thing in it—just Life</dc:contributor>
		<meta property="file-as" refines="#transcriber-6489">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-6489" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-6490">seen through a living       eye</dc:contributor>
