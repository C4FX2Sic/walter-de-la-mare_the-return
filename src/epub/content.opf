		<meta property="file-as" refines="#transcriber-5127">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-5127" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-5128">peeping between the blinds</dc:contributor>
		<meta property="file-as" refines="#transcriber-5128">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-5128" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-5129">looked down on the coachman</dc:contributor>
		<meta property="file-as" refines="#transcriber-5129">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-5129" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-5130">with       reins hanging loosely from his red squat-thumbed hand</dc:contributor>
		<meta property="file-as" refines="#transcriber-5130">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-5130" scheme="marc:relators">trc</meta>
