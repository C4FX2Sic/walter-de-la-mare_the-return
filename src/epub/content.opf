		<dc:contributor id="transcriber-596">‘that       you went out of this house</dc:contributor>
		<meta property="file-as" refines="#transcriber-596">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-596" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-597">just your usual self</dc:contributor>
		<meta property="file-as" refines="#transcriber-597">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-597" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-598">this afternoon</dc:contributor>
		<meta property="file-as" refines="#transcriber-598">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-598" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-599">for a       walk</dc:contributor>
		<meta property="file-as" refines="#transcriber-599">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-599" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-600">that for some reason you went to Widderstone—“to read the       tombstones</dc:contributor>
		<meta property="file-as" refines="#transcriber-600">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-600" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-601">” that you had a heart attack</dc:contributor>
		<meta property="file-as" refines="#transcriber-601">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-601" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-602">or</dc:contributor>
		<meta property="file-as" refines="#transcriber-602">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-602" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-603">as you said at first</dc:contributor>
