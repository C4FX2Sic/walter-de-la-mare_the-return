		<meta property="role" refines="#transcriber-2669" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-2670">dearest</dc:contributor>
		<meta property="file-as" refines="#transcriber-2670">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-2670" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-2671">’ he said in a low whisper</dc:contributor>
		<meta property="file-as" refines="#transcriber-2671">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-2671" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-2672">‘it’s only me</dc:contributor>
