		<meta property="file-as" refines="#transcriber-6412">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-6412" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-6413">yet... He listened       intently</dc:contributor>
