		<dc:contributor id="transcriber-8733">with head       slightly inclined</dc:contributor>
		<meta property="file-as" refines="#transcriber-8733">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-8733" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-8734">listening</dc:contributor>
		<meta property="file-as" refines="#transcriber-8734">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-8734" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-8735">as</dc:contributor>
		<meta property="file-as" refines="#transcriber-8735">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-8735" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-8736">with an interminable tardiness</dc:contributor>
