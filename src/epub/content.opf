		<meta property="role" refines="#transcriber-5197" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-5198">with chin uplifted</dc:contributor>
		<meta property="file-as" refines="#transcriber-5198">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-5198" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-5199">to the stairs. He bent forward a little</dc:contributor>
		<meta property="file-as" refines="#transcriber-5199">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-5199" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-5200">stood       motionless</dc:contributor>
		<meta property="file-as" refines="#transcriber-5200">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-5200" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-5201">drawn up</dc:contributor>
		<meta property="file-as" refines="#transcriber-5201">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-5201" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-5202">the pupils of his eyes slowly contracting</dc:contributor>
		<meta property="file-as" refines="#transcriber-5202">TRANSCRIBER_SORT</meta>
		<meta property="role" refines="#transcriber-5202" scheme="marc:relators">trc</meta>
		<dc:contributor id="transcriber-5203">expanding as he gazed down into the carpeted vacant gloom</dc:contributor>
